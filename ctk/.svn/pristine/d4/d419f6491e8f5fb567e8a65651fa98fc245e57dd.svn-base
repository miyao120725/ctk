<script>
    $(function(){
        //当滚动条的位置处于距顶部100像素以下时，跳转链接出现，否则消失
        $(function () {
            $(window).scroll(function() {
                $(window).scrollTop() > 100 ? $("#goTop").fadeIn(1500) : $("#goTop").fadeOut(1500);
            });
            //当点击跳转链接后，回到页面顶部位置
            $("#goTop").click(function(){
                $('body,html').animate({scrollTop:0},1000);
                return false;
            });
        });
    });
</script>
